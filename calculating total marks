#include <stdio.h>
int main() 
{
    int a, b, c, d, e, t;
    printf("enter the marks of a");
    scanf("%d", &a);
    printf("enter the value of b");
    scanf("%d", &b);
    printf("enter the value of c");
    scanf("%d", &c);
    printf("enter the value of d");
    scanf("%d", &d);
    printf("enter the value of e");
    scanf("%d", &e);
   //calculate the total marks
   t=(a+b+c+d+e);
   printf("total=%d", t);
    return 0;
}
