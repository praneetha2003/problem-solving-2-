#include <stdio.h>
#include <math.h>
int main()
{
    int l,w,h;
    float V;
    printf("enter the length of the pyramid\n");
    scanf("%d",&l);
    printf("enter the width of the pyramid\n");
    scanf("%d",&w);
    printf("enter the height of the pyramid\n");
    scanf("%d",&h);
    V=(l*w*h)/3;
    printf("the volume of the pyramid is:%f",V);

    return 0;
}
