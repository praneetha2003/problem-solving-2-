#include <stdio.h>
int main() 
{
    int l,b,h;
    printf("enterthe length of cuboid", l);
    scanf("%d", &l);
    printf("enter the breadth of cuboid", b);
    scanf("%d", &b);
    printf("enter the height of cuboid", h);
    scanf("%d", &h);
    int volume;
    volume=(l*b*h);
    printf("the volume of cuboid is %d", volume);
    return 0;
}
